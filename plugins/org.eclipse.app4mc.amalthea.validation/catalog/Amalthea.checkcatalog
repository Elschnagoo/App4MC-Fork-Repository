<?xml version="1.0" encoding="UTF-8"?>
<cc:Catalog xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:cc="http://www.eclipse.org/sphinx/check/catalog"
    id="ValidationRules"
    label="Amalthea Validation Rules">

  <categories
    id="Basic"
    label="Basic Model Checks"/>

  <categories
    id="MappingModel"
    label="Mapping Model Checks"/>

  <categories
    id="EventModel"
    label="Event Model Checks"/>

  <categories
    id="ConstraintsModel"
    label="Constraints Model Checks"/>

  <categories
    id="HardwareModel"
    label="Hardware Model Checks"/>
   
  <categories
    id="SoftwareModel"
    label="Software Model Checks"/>
   

  <constraints
    id="DuplicateUniqueName"
    label="Duplicate Unique Name"
    description="Checks for duplicate unique names of IReferable objects"
    severity="Error"
    message="{0}: duplicate name &quot;{1}&quot;"
    categories="Basic"/>

  <constraints
    id="MissingReferableName"
    label="Missing Referable Name"
    description="Checks for missing names of IReferable objects"
    severity="Error"
    message="{0}: missing name"
    categories="Basic"/>

  <constraints
    id="CustomPropertyDuplicateKey"
    label="Custom Property Duplicate Key"
    description="Checks for duplicate property keys"
    severity="Warning"
    message="CustomProperty: duplicate key &quot;{2}&quot;"
    categories="Basic"/>

  <constraints
    id="UndefinedUnit"
    label="Undefined Unit"
    description="Checks for undefined units"
    severity="Error"
    message="{0}: undefined unit ( in &quot;{1}&quot; )"
    categories="Basic"/>
    
  <constraints
    id="TimeRangeExceeded"
    label="Time Range Exceeded"
    description="Checks for Time values that are negative or zero"
    severity="Error"
    message="{0}: {2} value must be {3} ( in &quot;{1}&quot; )"
    categories="Basic"/>

  <constraints
    id="WeibullParameters"
    label="Weibull Parameters"
    description="Checks if the parameters of the Weibul distributions are valid"
    severity="Warning"
    message="Distribution contains invalid Weibull parameters: lowerBound: (long) {0} upperBound: (long) {1}"
    categories="Basic"/>


  <constraints
    id="Process2Scheduler2CoreMapping"
    label=""
    severity="Error"
    message="{0}"
    categories="MappingModel"/>

  <constraints
    id="LabelEvent"
    label="Label Event Checks"
    description="Checks on LabelEvent for complete information"
    severity="Error"
    message="LabelEvent: {0}"
    categories="EventModel"/>

  <constraints
    id="EventChain"
    label="Event Chains Checks"
    description="Checks on EventChains for correctly chained response/stimulus pairs"
    severity="Error"
    message="&quot;{0}&quot;"
    categories="ConstraintsModel"/>

  <constraints
    id="SystemStructure"
    label="System Structure Checks"
    description="Checks on HwModel for ..."
    severity="Error"
    message="&quot;{0}&quot;"
    categories="HardwareModel"/>

  <constraints
    id="HardwareConnection"
    label="Hardware Connection Checks"
    description="Checks the correctness of HwConnection definitions"
    severity="Error"
    message="HW Connection &quot;{0}&quot;: {1}"
    categories="HardwareModel"/>

  <constraints
    id="HardwarePort"
    label="Hardware Port Checks"
    description="Checks the correctness of HwPort definitions"
    severity="Error"
    message="HW Port &quot;{0}&quot;: {1}"
    categories="HardwareModel"/>

  <constraints
    id="HardwareAccessPath"
    label="Hardware Access Path Checks"
    description="Checks the correctness of HwAccessPath definitions"
    severity="Error"
    message="HW Access Path &quot;{0}&quot;: {1}"
    categories="HardwareModel"/>

  <constraints
    id="UndefinedHardware"
    label="Undefined Hardware "
    description="Checks for missing definitions"
    severity="Warning"
    message="Hardware model: {0}"
    categories="HardwareModel"/>

  <constraints
    id="DataDependency"
    label="Data Dependency Checks"
    description="Checks the correctness of the &quot;depends on&quot; references"
    severity="Error"
    message="Data dependency: {0}"
    categories="SoftwareModel"/>

  <constraints
    id="CallArgument"
    label="Call Argument Checks"
    description="Checks the correctness of CallArguments definitions"
    severity="Error"
    message="Call argument: {0}"
    categories="SoftwareModel"/>

</cc:Catalog>
