<?xml version="1.0" encoding="UTF-8"?>
<cc:Catalog xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:cc="http://www.eclipse.org/sphinx/check/catalog"
    id="ValidationRules"
    label="Amalthea Validation Rules">

  <categories
    id="Basic"
    label="Basic Model Checks"/>

  <categories
    id="WeibullParametersConstraint"
    label="Weibull Parameter Bound"/>

  <categories
    id="MappingModelConstraint"
    label="MappingModel Constraint(Process->Scheduler->Core)"/>

  <categories
    id="EventModelConstraint"
    label="Event Model Checks"/>

  <categories
    id="ConstraintsModelConstraint"
    label="EventChain Consistency"/>

  <categories
    id="HardwareModelConstraint"
    label="Custom Attribute/Feature Duplicate Key"/>


  <constraints
    id="DuplicateUniqueName"
    label="Duplicate Unique Name"
    description="Checks for duplicate unique names of IReferable objects"
    severity="Error"
    message="Duplicate name found: {0}"
    categories="Basic"/>

  <constraints
    id="MissingReferableName"
    label="Missing Referable Name"
    description="Checks for missing names of IReferable objects"
    severity="Error"
    message="{0}: missing name"
    categories="Basic"/>

  <constraints
    id="CustomPropertyDuplicateKey"
    label="Custom Property Duplicate Key"
    description="Checks for duplicate property keys"
    message="{0} - Duplicate property found: {1}"
    categories="Basic"/>

  <constraints
    id="UndefinedUnit"
    label="Undefined Unit"
    description="Checks for undefined units"
    severity="Error"
    message="{0}: undefined unit"
    categories="Basic"/>

  <constraints
    id="WeibullParametersConstraint"
    label="Weibull Parameter Constraint"
    description="Checks if the values of a component's parameters are valid"
    severity="Warning"
    message="Deviation Dist contains invalid Weibull parameters: lowerBound: (long) {0} upperBound: (long) {1}"
    categories="WeibullParametersConstraint"/>

  <constraints
    id="Process2Scheduler2CoreMappingConstraint"
    label=""
    severity="Error"
    categories="MappingModelConstraint"
    message="{0}"/>

  <constraints
    id="LabelEventConstraint"
    label="LabelEvent Constraint"
    description="Checks on LabelEvent for complete information"
    severity="Error"
    message="LabelEvent: {0}"
    categories="EventModelConstraint"/>

  <constraints
    id="ProcessEventConstraint"
    label="ProcessEvent Constraint"
    description="Checks on ProcessEvent for complete information"
    severity="Error"
    message="ProcessEvent: {0}"
    categories="EventModelConstraint"/>

  <constraints
    id="RunnableEventConstraint"
    label="RunnableEvent Constraint"
    description="Checks on RunnableEvent for complete information"
    severity="Error"
    message="RunnableEvent: {0}"
    categories="EventModelConstraint"/>

  <constraints
    id="SemaphoreEventConstraint"
    label="SemaphoreEvent Constraint"
    description="Checks on SemaphoreEvent for complete information"
    severity="Error"
    message="SemaphoreEvent: {0}"
    categories="EventModelConstraint"/>

  <constraints
    id="StimulusEventConstraint"
    label="StimulusEvent Constraint"
    description="Checks on StimulusEvent for complete information"
    severity="Error"
    message="StimulusEvent: {0}"
    categories="EventModelConstraint"/>

  <constraints
    id="EventChainsConstraint"
    label="EventChains Constraint"
    description="Checks on EventChains for correctly chained response/stimulus pairs"
    severity="Error"
    message="&quot;{0}&quot;"
    categories="ConstraintsModelConstraint"/>

  <constraints
    id="HwModelAbstractTypeDuplicateNameConstriant"
    label="HwModel AbstractType Duplicate Name Constraint"
    description="Checks on HwModel for duplicate attribute/feature name (key) in Abstract Types like CoreType, ECUType, System"
    severity="Error"
    message="&quot;{0}&quot;"
    categories="HardwareModelConstraint"/>

</cc:Catalog>
