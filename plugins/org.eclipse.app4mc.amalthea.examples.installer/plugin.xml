               name="app4mc.example.hardware"
               contentURI="examples/app4mc.example.hardware/"
               description="%_UI_AMALTHEA_Example_Hwmodel_Project_desc"/>
      </example>
      <example
            id="AMALTHEA NumericMode Project"
            pageImage="icons/full/wizban/NewEMFExample.gif"
