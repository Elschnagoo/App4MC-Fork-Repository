      <category
            id="org.eclipse.app4mc.tools.Examples"
            name="%_UI_Tools_ExamplesCategory_name"
            parentCategory="org.eclipse.ui.Examples">
      </category>
      <wizard
            id="org.eclipse.app4mc.tools.examples.JavaExamplesWizard"
            name="%_UI_Tools_JavaExamplesWizard_name"
