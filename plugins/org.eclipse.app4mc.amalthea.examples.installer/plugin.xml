            id="AMALTHEA Democar Projects"
            wizardID="org.eclipse.app4mc.amalthea.examples.DemocarExamplesWizard"
            pageImage="icons/full/wizban/NewEMFExample.gif">
            <projectDescriptor
               name="app4mc.example.democar"
               contentURI="examples/app4mc.example.democar/"
               description="%_UI_AMALTHEA_Example_Democar_Project_desc"/>
            <projectDescriptor
               name="app4mc.example.democar.split"
               contentURI="examples/app4mc.example.democar.split/"
               description="%_UI_AMALTHEA_Example_Democar_Project_desc"/>
      </example>
      <example
            id="AMALTHEA Modeling Projects"
            pageImage="icons/full/wizban/NewEMFExample.gif"
            wizardID="org.eclipse.app4mc.amalthea.examples.ModelingExamplesWizard">
               <projectDescriptor
                     name="app4mc.example.modeling"
                     contentURI="examples/app4mc.example.modeling/"
                     description="%_UI_AMALTHEA_Example_Modeling_Project_desc"/>
               <projectDescriptor
                     name="app4mc.example.modeling.periodic"
