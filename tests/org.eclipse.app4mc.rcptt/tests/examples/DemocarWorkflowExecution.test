------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Model Explorer" | get-tree | select "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow/sample_workflow.js"
get-view "Model Explorer" | get-tree 
    | select "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow/sample_workflow.js" 
    | get-menu "Run As/1 EASE Script" | click
//Hack to wait 15 sec till EASE workflow is finished
wait 15000
//checking for available results
get-view "Model Explorer" | get-tree | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output" 
    | get-property caption | equals "workflow-output" | verify-true
with [get-view "Model Explorer" | get-tree] {
    get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output" | get-property caption 
        | equals "workflow-output" | verify-true
    get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output" | get-property childCount | equals 4 
        | verify-true
}
get-view "Model Explorer" | get-tree | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output/create" 
    | get-property caption | equals create | verify-true
get-view "Model Explorer" | get-tree 
    | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output/create/createtasks.amxmi" 
    | get-property caption | equals "createtasks.amxmi" | verify-true
get-view "Model Explorer" | get-tree 
    | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output/dfg/mapping_dfg.amxmi" 
    | get-property caption | equals "mapping_dfg.amxmi" | verify-true
get-view "Model Explorer" | get-tree 
    | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output/ga/mapping_galb.amxmi" 
    | get-property caption | equals "mapping_galb.amxmi" | verify-true
get-view "Model Explorer" | get-tree 
    | get-item "org.eclipse.app4mc.amalthea.example.democar.mapping/workflow-output/ilp/mapping_ilplb.amxmi" 
    | get-property caption | equals "mapping_ilplb.amxmi" | verify-true
