Content-Type: text/ecl
Entry-Name: .content

get-view "Model Explorer" | get-tree | select testproject | get-menu "New/AMALTHEA Model" | click
get-window "New Model File" | get-combo -after [get-label Metamodel] | select "AMALTHEA Model"
with [get-window "New Model File" | get-combo -after [get-label "Root element package"]] {
    get-property "getItemCount()" | equals 10 | verify-true
}
with [get-window "New Model File"] {
    get-combo -after [get-label "Root element package"] | select central
    get-combo -after [get-label "Root element class"] | select AMALTHEA
    get-button "Next >" | click
    get-button Finish | click
}
with [get-editor "default.amxmi" | get-section "default.amxmi Contents"] {
    with [get-label "default.amxmi Contents"] {
        get-property className | equals "org.eclipse.swt.widgets.Label" | verify-true
        get-property caption | equals "default.amxmi Contents" | verify-true
    }
    with [get-tree] {
        get-item AMALTHEA | get-property caption | equals AMALTHEA | verify-true
        get-item AMALTHEA | get-property childCount | equals 0 | verify-true
    }
}
get-editor "default.amxmi" | get-section "default.amxmi Contents" | get-tree | select AMALTHEA
get-view Properties | get-label AMALTHEA | get-property caption | equals AMALTHEA | verify-true
with [get-view "Model Explorer" | get-tree] {
    get-item "testproject/default.amxmi" | get-property caption | equals "default.amxmi" | verify-true
    get-item "testproject/default.amxmi" | get-property childCount | equals 1 | verify-true
    get-item "testproject/default.amxmi/AMALTHEA" | get-property caption | equals AMALTHEA | verify-true
    get-item "testproject/default.amxmi/AMALTHEA" | get-property childCount | equals 0 | verify-true
}
