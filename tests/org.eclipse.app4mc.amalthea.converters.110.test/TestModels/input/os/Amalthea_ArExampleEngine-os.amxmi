<os:OSModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:os="http://www.amalthea.itea2.org/model/1.1.0/os">
  <osBuffering/>
  <scheduler name="Task_Scheduler_Core_1">
    <schedulingAlgorithm xsi:type="os:OSEK"/>
    <schedulingUnit xsi:type="os:SchedulingHWUnit">
      <delay value="1" unit="ns"/>
    </schedulingUnit>
  </scheduler>
  <scheduler name="Task_Scheduler_Core_2">
    <schedulingAlgorithm xsi:type="os:OSEK"/>
    <schedulingUnit xsi:type="os:SchedulingHWUnit">
      <delay value="1" unit="ns"/>
    </schedulingUnit>
  </scheduler>
</os:OSModel>
